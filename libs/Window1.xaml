<Window x:Class="libs.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:libs"
        mc:Ignorable="d"
        Title="Window1" Height="250" Width="250">
    <Grid>
        <TextBox x:Name="NamePeople" HorizontalAlignment="Center" Margin="0,37,0,0" TextWrapping="Wrap" Text="Имя" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="AgePeople" HorizontalAlignment="Center" Margin="0,81,0,0" TextWrapping="Wrap" Text="Возраст" VerticalAlignment="Top" Width="120"/>
        <Button Content="Конверт" HorizontalAlignment="Center" Margin="0,151,0,0" VerticalAlignment="Top" Height="18" Width="120" Click="Button_Click"/>
        <Button Content="Деконверт" HorizontalAlignment="Center" Margin="0,182,0,0" VerticalAlignment="Top" Height="18" Width="120" Click="Button_Click1"/>

    </Grid>
</Window>
